    "format:staged": "lint-staged",
    "format:js": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "format:styles": "prettier --write \"**/*.{css,scss,sass}\"",
    "format:docs": "prettier --write \"**/*.{md,mdx}\"",
    "format:config": "prettier --write \"**/*.{json,yml,yaml}\"",
